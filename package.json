{
  "name": "Trails",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build:all": "npm run build:css && npm run build:images && npm run build:js",
    "build:css": "gulp css",
    "build:images": "cpx \"node_modules/leaflet*/dist/images/*.{gif,png,svg}\" public/assets/lib/leaflet/",
    "build:js": "browserify public/app/app.js -o public/app/main.js",
    "postinstall": "npm run build:all",
    "start": "nf start",
    "test:server": "nf -e .env,.env-test run mocha -- --recursive test/server",
    "test:file": "nf -e .env,.env-test run mocha -- --recursive",
    "watch:js": "watchify public/app/app.js -o public/app/main.js --iw node_modules --iw server -v"
  },
  "dependencies": {
    "angular": "^1.5.8",
    "angular-jwt": "^0.1.9",
    "angular-messages": "^1.5.8",
    "angular-sanitize": "^1.5.8",
    "angular-simple-logger": "^0.1.7",
    "angular-ui-bootstrap": "^2.2.0",
    "bcryptjs": "^2.4.0",
    "body-parser": "^1.15.2",
    "browserify": "^14.0.0",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "chai-http": "^4.3.0",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.1",
    "cpx": "^1.5.0",
    "debug": "^2.3.2",
    "express": "^4.14.0",
    "foreman": "^2.0.0",
    "generate-schema": "^2.3.1",
    "gulp": "^3.9.1",
    "gulp-concat-css": "^2.3.0",
    "jsonwebtoken": "^7.2.1",
    "karma": "^1.3.0",
    "karma-mocha": "^1.3.0",
    "leaflet": "^1.0.1",
    "leaflet-control-geocoder": "^1.5.4",
    "leaflet-draw": "^0.4.0",
    "mocha": "^3.1.2",
    "mongoose": "^4.6.8",
    "morgan": "^1.7.0",
    "passport": "^0.3.2",
    "passport-jwt": "^2.2.1",
    "request": "^2.78.0",
    "togpx": "^0.5.3",
    "tokml": "^0.4.0",
    "ui-leaflet": "^2.0.0",
    "ui-leaflet-draw": "^0.0.3",
    "vkbeautify": "^0.99.1",
    "watchify": "^3.8.0",
    "winston": "^2.3.0"
  },
  "license": "MIT",
  "devDependencies": {
    "eslint": "^3.8.1",
    "eslint-config-airbnb": "^14.0.0",
    "eslint-plugin-import": "^2.0.1",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-react": "^6.4.1"
  }
}
